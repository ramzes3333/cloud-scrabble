{
  "/board-manager-service/*": {
    "target": "http://localhost:8085",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/board-manager-service": "/board-manager-service"
    }
  },
  "/board-manager-websocket/*": {
    "target": "http://localhost:8085",
    "secure": false,
    "changeOrigin": true,
    "ws": true,
    "pathRewrite": {
      "^/board-manager-websocket": "/board-manager-websocket"
    }
  },
  "/game-service/*": {
    "target": "http://localhost:8085",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/game-service": "/game-service"
    }
  },
  "/tile-manager-service/*": {
    "target": "http://localhost:8085",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/tile-manager-service": "/tile-manager-service"
    }
  },
  "/external-services/*": {
    "target": "http://localhost:8085",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/external-services": "/external-services"
    }
  }
}
